{
  "name": "express-browsersupport",
  "version": "1.3.4",
  "description": "A way to define which browsers are supported in your express app",
  "main": "src/browsersupport.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha -R spec --recursive",
    "test:watch": "mocha -R spec --recursive --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint -c ./package.json ./src"
  },
  "author": {
    "name": "Glenn de Haan",
    "email": "glenn@dehaan.cloud",
    "url": "https://glenndehaan.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/glenndehaan/express-browsersupport.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "indent": [
        "error",
        4
      ]
    },
    "extends": [
      "eslint:recommended"
    ]
  },
  "eslintIgnore": [
    "*.test.js"
  ],
  "dependencies": {
    "express-useragent": "^1.0.12"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "express": "^4.16.3",
    "mocha": "^5.1.0",
    "nyc": "^11.6.0",
    "request": "^2.85.0",
    "should": "^13.1.1"
  }
}
